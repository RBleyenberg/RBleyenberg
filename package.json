{
  "name": "dare-business-software",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --prefix-colors white.bgBlue,white.bgRed --names angular,json-server --kill-others \"npm run serve-proxy\" \"npm run start-auth\"",
    "serve": "ng serve",
    "serve-proxy": "ng serve --proxy-config proxy.conf.json",
    "json-server": "json-server --watch data/db.json --routes data/routes.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "8.0.0",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@briebug/ngrx-auto-entity": "^0.4.2",
    "@ngrx/effects": "^8.0.0",
    "@ngrx/router-store": "^8.0.0",
    "@ngrx/store": "^8.0.0",
    "@ngx-translate/core": "11.0.1",
    "@types/prismjs": "1.16.0",
    "chart.js": "2.8.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.7",
    "d3": "5.9.2",
    "firebase": "^7.13.1",
    "hammerjs": "2.0.8",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "moment": "2.24.0",
    "ng2-charts": "2.2.4",
    "ngx-color-picker": "7.5.0",
    "ngx-cookie-service": "2.2.0",
    "perfect-scrollbar": "1.4.0",
    "prismjs": "1.23.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.5.2",
    "uuidv4": "^5.0.1",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@ngrx/store-devtools": "^7.4.0",
    "@types/jest": "^24.0.25",
    "@types/node": "~10.9.4",
    "acorn": "6.1.1",
    "codelyzer": "^5.1.0",
    "concurrently": "^5.1.0",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "jasmine-marbles": "^0.5.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.2",
    "jest-junit": "^7.0.0",
    "jest-preset-angular": "^7.1.1",
    "json-server": "^0.15.0",
    "lint-staged": "^7.2.0",
    "ng-packagr": "^5.1.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "ts-jest": "^24.0.2",
    "ts-node": "~5.0.1",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~3.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts)?(x)"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.module.ts",
      "!src/main.ts",
      "!src/polyfills.ts",
      "!src/environments/*.ts",
      "projects/ngrx-auto-entity/src/lib/**/*.ts",
      "!projects/ngrx-auto-entity/src/lib/**/*.module.ts",
      "!projects/ngrx-auto-entity/src/lib/index.ts",
      "!<rootDir>/setup-jest.ts",
      "!<rootDir>/jestGlobalMocks.ts"
    ],
    "moduleNameMapper": {
      "^ngrx-auto-entity/(.*)": "<rootDir>/projects/ngrx-auto-entity/$1",
      "^state/(.*)": "<rootDir>/src/app/state/$1",
      "^core/(.*)": "<rootDir>/src/app/core/$1",
      "^models/(.*)": "<rootDir>/src/app/models/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  }
}
